# Environment Configuration for Review Service
NODE_ENV=development
ENABLE_APM=0

# JWT Tokens
JWT_TOKEN=your_jwt_token_here
GATEWAY_JWT_TOKEN=your_gateway_jwt_token_here

# RabbitMQ Configuration
RABBITMQ_ENDPOINT=amqp://username:password@localhost:5672

# Database Configuration
DATABASE_HOST=localhost
DATABASE_USER=username
DATABASE_PASSWORD=password
DATABASE_NAME=database_name

# Cloudinary Configuration
CLOUD_NAME=your_cloud_name
CLOUD_API_KEY=your_cloud_api_key
CLOUD_API_SECRET=your_cloud_api_secret

# API Gateway Configuration
API_GATEWAY_URL=http://localhost:4000

# Elasticsearch Configuration
ELASTIC_SEARCH_URL=http://localhost:9200

# Cluster Configuration
CLUSTER_TYPE=development

# APM Configuration (if enabled)
ELASTIC_APM_SERVER_URL=http://localhost:8200
ELASTIC_APM_SECRET_TOKEN=your_apm_secret_token 